.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #fff;
}

.search-view {
  left: 0;
  right: 0;
  top: 0;
  z-index: 500;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  background: #fff;
}

.search-content {
  background: white;
  width: 700rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: auto;
  padding-left: 20rpx;
  padding-bottom: auto;
  margin: auto;
  height: 80rpx;
  border-radius: 25px;
  -moz-box-shadow: 2px 2px 10px #c9c9c9;
  -webkit-box-shadow: 2px 2px 10px #c9c9c9;
  box-shadow: 2px 2px 10px #c9c9c9;
}

.search-icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.search-input {
  width: 500rpx;
  font-size: 28rpx;
}

.search-btn {
  width: 155rpx;
  height: 80rpx;
  margin-right: 0rpx;
  background-color: #41b5a4;
  font-size: 28rpx;
  align-items: center;
  border-top-left-radius: 0rpx;
  border-bottom-left-radius: 0rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  border: none;
  position: relative;
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
}

swiper {
  height: calc(100vh - 100rpx);
  overflow: auto;
  background: #f5f5f5;
}

swiper-item {
  overflow: auto !important;
}

.list-tab {
  background: #fff;
  width: 100%;
  height: 65rpx;
  border-bottom: 1px solid #eee;
  justify-content: space-around;
  z-index: 1;
  position: fixed;
  margin-top: 100rpx;
}

.interest{
  z-index: 1;
  font-size: 50rpx;
  font-weight: bold;
  height: 100rpx;
}

.interest-content{
  z-index: 1;
  font-size: 30rpx;
  height: 100rpx;
}

.list-tab view {
  width: 100rpx;
  font-size: 32rpx;
  text-align: center;
  color: #999;
}

.list-tab .checked-item {
  border-bottom: 6rpx solid #3bbab0;
  color: #3bbab0;
  font-size: 32rpx;
  line-height: 65rpx;
  width: 100rpx;
  font-weight: bold;
}

/**
*人才
*/

.content-item {
  display: flex;
  padding: 20rpx;
  flex-direction: column;
  margin-top: 100rpx;
  z-index: 1;
}
.content-itemType {
  display: flex;
  padding: 20rpx;
  flex-direction: column;
  margin-top: 40rpx;
  z-index: 1;

}

.content-hot {
  font-size: 24rpx;
  color: #333;
}

.content-item-msg {
  background: #fff;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  height: 193rpx;
  border-radius: 8rpx;
  padding-bottom: 10rpx
}

.content-msg {
  display: flex;
  flex-direction: row;
  margin: 40rpx 20rpx 0rpx 20rpx;
  align-items: center;
}

.item-msg-image {
  max-width: 106rpx;
  max-height: 106rpx;
  border-radius: 25px;
}

.item-msg-msg {
  width: 350rpx;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.msg-name {
  font-size: 28rpx;
  color: #333;
  font-weight: bold;
}

.msg-fields {
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #999;
}

.item-msg-num {
  margin-left: 20rpx;
  font-size: 66rpx;
  color: #666;
  font-weight: bold;
  width: 200rpx;
  text-align: right;
}

.content-msg-bottom {
  align-items: center;
  display: flex;
  flex-direction: row;
  background: #f9f9f9;
  border-radius: 20rpx;
  margin-left: 20rpx;
  margin-right: auto;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-top: 20rpx;
height: 40rpx
}

.content-msg-image {
  width: 20rpx;
  height: 24rpx;
}

.content-msg-school {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #666; 
}

.university-msg-name {
  font-size: 40rpx;
  color: #333;
  font-weight: bold;
  margin-left: 20rpx;
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.fields-item-msg {
  margin-top: 20rpx;
  background: #fff;
  display: flex;
  height: 160rpx;
  flex-direction: row;
  border-radius: 8rpx;
}

.fieldst-msg-lef {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;

  justify-content: center
}
.left-msg{
  display: flex;
  flex-direction: column;
  width: 60%;
}
.left-name {
  font-size: 40rpx;
  color: #333;
  margin-left: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  font-weight: 700
}

.left-container {
  align-items: center;
  margin-top: 20rpx;
  width: 130rpx;
  height: 38rpx;
  border: 2rpx solid #3bbab0;
  margin-left: 20rpx;
  border-radius: 6rpx;
  line-height: 38rpx;
}

.left-container-text {
  font-size: 22rpx;
  color: #3bbab0;
  justify-content: center;
  display: flex;
}

.right-num {
  width: 40%;
  text-align: right;
  margin-right: 20rpx;
  font-size: 66rpx;
  color: #666;
  font-weight: bold;
  align-self: center;
}

.left-title {
  display: flex;
  flex-direction: row;
  width: 400rpx;
  align-items: center;
}

.back-color {
  background: #3bbab0;
  border-radius: 4rpx;
  width: 6rpx;
  height: 28rpx;
}

.content-item-show {
  height: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.item-show-image {
  margin-top: 240rpx;
  width: 500rpx;
  height: 370rpx;
}

.item-show-text {
  margin-top: 50rpx;
  color: #999;
  font-size: 24rpx;
}

.header {
  width: 65%;
  margin: 0.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
