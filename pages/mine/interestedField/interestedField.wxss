.tabTit {
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: solid 1px #eee;
  width: 750rpx;
  z-index: 1;
  background: #fff;
  font-size: 15px;
  text-align: center;
  position: fixed;
  margin-top: 166rpx;
}
.tabTitType {
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: solid 1px #eee;
  width: 750rpx;
  z-index: 1;
  background: #fff;
  font-size: 15px;
  text-align: center;
  position: fixed;
  margin-top: 100rpx;
}

.tab-text {
  display: flex;
  width: 100%;
  justify-content: center;
}

.tab-name {
  font-size: 24rpx;
  width: 100rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.tabTit .active {
  color: #e64340;
  background: #fff;
}

.tabTit .active image {
  transform: rotate(180deg);
  background: none;
}

.tabTit image {
  width: 26rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-left: 5px;
  align-self: center;
}
.tabTitType .active {
  color: #e64340;
