/* pages/firstField/firstField.wxss */

.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  margin: 0;
  padding: 0;
  background: #fafafa;
}

.list-tabTop {
  padding-right: 100rpx;
  padding-left: 100rpx;
  width: 100%;
  height: 65rpx;
  border-bottom: 1px solid #eee;
  justify-content: space-around;
  text-align: center;
}

.navbarTop {
  color: #999;
}

.list-tabTop view {
  width: 33.33%;
  font-size: 32rpx;
  line-height: 65rpx;
}

.list-tabTop .checked-itemTop {
  color: #3bbab0;
  font-size: 32rpx;
  line-height: 65rpx;
  width: 33.33%;
  font-weight: 600;
}

.hideBack {
  display: flex;
  width: 100%;
  height: 100%;
  background: #fff;
  flex-direction: column;
  align-items: center;
}

.hide-image {
  padding-top: 260rpx;
  width: 396rpx;
  height: 368rpx;
}

.hide-text {
  margin-top: 50rpx;
  font-size: 32rpx;
  color: #333;
}

.type-navbar-item {
  color: #999;
}

.tab-wrapper {
  /* padding-left: 40rpx; 
  padding-right: 40rpx; */
  white-space: nowrap;
  height: 68rpx;
  width: 100%;
  background-color: #fff;
}

.type-box {
  width: 150rpx;
  box-sizing: content-box;
  font-size: 28rpx;
  line-height: 64rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}

.tab-active {
  display: inline-block;
  border-bottom: 4rpx solid #3bbab0;
  color: #3bbab0;
  font-weight: 600;
}

.content-wrapper {
  width: 100%;
  flex: 1;
}

.tab-menu {
  position: fixed;
  width: 100%;
  background: #fff;
  border-bottom: #eee solid 1px;
  z-index: 100000;
}

.list-tab {
  width: 100%;
  height: 66rpx;
  display: flex;
  justify-content: space-between;
  padding-left: 120rpx;
  padding-right: 145rpx;
}

.list-tab view {
  font-size: 28rpx;
  color: #999;
  line-height: 64rpx;
}

.list-tab .checked-item {
  color: #333;
  font-weight: 500;
  text-align: center;
  line-height: 60rpx;
  align-self: center;
}

.swipperTop {
  height: calc(100vh - 67rpx);
  overflow: auto;
}

.swiperTop-item {
  overflow: auto !important;
}

swiper {
  height: calc(100vh - 218rpx);
  overflow: auto;
}

swiper-item {
  overflow: auto !important;
}

/******************************子领域排名******************************/

.investment-small-list {
}

.investment-small-list view:nth-last-child(1) {
  border-bottom: 0;
}

.investment-small-list .item {
  display: flex;
  flex-direction: column;
  width: 100%;
  padding-bottom: 10rpx;
  padding-top: 10rpx;
  border-bottom: 10rpx solid #f5f5f5;
}

/* 二级领域--信息 */

.field-content {
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 10rpx;
  width: 100%;
  display: flex;
}

.field-name {
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  width: 350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.field-sum {
  font-size: 32rpx;
  color: #d26161;
  font-weight: bold;
  width: 150rpx;
}

.field-num1-true {
  margin-left: 5rpx;
  font-size: 32rpx;
  color: #d26161;
  font-weight: bold;
  width: 130rpx;
}

.field-num1-false {
  margin-left: 23rpx;
  font-size: 32rpx;
  color: #48c3af;
  font-weight: bold;
}

.field-more {
  align-self: center;
  font-size: 24rpx;
  color: #999;
}

.field-img {
  margin-left: 10rpx;
  align-self: center;
  width: 15rpx;
  height: 20rpx;
}

/* 标签 */

.person-icon {
  display: flex;
  width: 100%;
}

.icon-left {
  margin-top: 8rpx;
  padding: 1rpx 8rpx 1rpx 8rpx;
