
.paper-items {
  display: flex;
  flex-direction: column;
  background: #fff
}
.paper-items-msg{
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}
.paper-items-name {
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
    overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.paper-items-con {
  margin-top: 20rpx;
  font-size: 26rpx;
  color: #999;
    overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.items-num{
  display: flex;
  margin-top: 20rpx;
  flex-direction: row;
justify-content: flex-end;
align-items: center;
}
.num-title{
    font-size: 26rpx;
  color: #999;
  }
  .num-text{
    font-size: 26rpx;
  color: #41B5A4;
  }
.paper-items-back-10 {
  background: #f5f5f5;
  height: 20rpx;
}