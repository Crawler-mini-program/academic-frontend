.container{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  background: #fff;
}
.content-msg{
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  height: 100%;
}
.title{
  color: #333;
  font-size: 40rpx;
  font-weight: bold
}
.project-msg{
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 60rpx;
  width: 100%
}
.project-title{
  font-size: 28rpx;
  color: #333;
}
.project-title-{
  font-size: 28rpx;
  color: #333;
  width: 35%;
}
.project-text{
  margin-left: 20rpx;
    font-size: 28rpx;
  color: #333;
}
.project-text-{
    font-size: 28rpx;
  color: #333;
  width: 100%;
}

.bottom{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  bottom: 0;
  background: #f9f9f9;
}
.bottom-left{
  float: left;
  width: 100%;
  display: flex;
  height: 80rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background: #f9f9f9;
  border-radius: 0;
}
.bottom-left::after {
  border: none;
}
.bottom-left-image{
  width: 28rpx;
  height: 28rpx;
}
.bottom-left-text {
  margin-left: 6rpx;
  font-size: 28rpx;
  color: #333;
}

.patent-items-back-1 {
